                    let awayTeam;
                    do {
                        awayTeam = randomLeague.teams[Math.floor(Math.random() * randomLeague.teams.length)];
                    } while (homeTeam === awayTeam);
                    
                    const predictionTypes = ['Home Win & BTTS', 'Away Win', 'Draw & BTTS', 'Over 2.5 Goals', 'BTTS - Yes'];
