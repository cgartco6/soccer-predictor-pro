        // Set up navigation between sections
        function setupNavigation() {
            const navLinks = document.querySelectorAll('.nav-link[data-section]');
            const contentSections = document.querySelectorAll('.content-section');
            
            navLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    const targetSection = this.getAttribute('data-section');
                    
                    // Update active nav link
                    navLinks.forEach(nav => nav.classList.remove('active'));
                    this.classList.add('active');
                    
                    // Show target section
                    contentSections.forEach(section => {
                        section.classList.remove('active');
                    });
                    document.getElementById(targetSection).classList.add('active');
                    
                    // Update app state
                    appState.currentSection = targetSection;
                    
                    // Special handling for certain sections
                    if (targetSection === 'analytics') {
                        updateAnalyticsCharts();
                    }
                });
            });
            
            // Quick action buttons
            const quickActionButtons = document.querySelectorAll('button[data-section]');
            quickActionButtons.forEach(button => {
                button.addEventListener('click', function() {
                    const targetSection = this.getAttribute('data-section');
                    const navLink = document.querySelector(`.nav-link[data-section="${targetSection}"]`);
                    if (navLink) {
                        navLink.click();
                    }
                });
            });
        }

        // Set up event listeners for various interactions
        function setupEventListeners() {
            // Save team button
            document.getElementById('saveTeamBtn').addEventListener('click', function() {
                const teamName = document.getElementById('teamName').value;
                const teamCountry = document.getElementById('teamCountry').value;
                const teamLeague = document.getElementById('teamLeague').value;
                const teamLogo = document.getElementById('teamLogo').value;
                
                if (teamName && teamCountry && teamLeague) {
                    addTeam(teamName, teamCountry, teamLeague, teamLogo);
                    // Close modal
                    const modal = bootstrap.Modal.getInstance(document.getElementById('addTeamModal'));
                    modal.hide();
                    // Reset form
                    document.getElementById('addTeamForm').reset();
                } else {
                    alert('Please fill in all required fields');
                }
            });
            
            // Save match button
            document.getElementById('saveMatchBtn').addEventListener('click', function() {
                const homeTeam = document.getElementById('homeTeam').value;
                const awayTeam = document.getElementById('awayTeam').value;
                const matchLeague = document.getElementById('matchLeague').value;
                const matchDate = document.getElementById('matchDate').value;
                
                if (homeTeam && awayTeam && matchLeague && matchDate) {
                    addMatchPrediction(homeTeam, awayTeam, matchLeague, matchDate);
                    // Close modal
                    const modal = bootstrap.Modal.getInstance(document.getElementById('addMatchModal'));
                    modal.hide();
                    // Reset form
                    document.getElementById('addMatchForm').reset();
                } else {
                    alert('Please fill in all required fields');
                }
            });
            
            // Refresh data button
            document.getElementById('refreshDataBtn').addEventListener('click', refreshData);
            document.getElementById('manualRefreshBtn').addEventListener('click', refreshData);
            
            // Team search and filters
            document.getElementById('teamSearch').addEventListener('input', filterTeams);
            document.getElementById('leagueFilter').addEventListener('change', filterTeams);
            document.getElementById('countryFilter').addEventListener('change', filterTeams);
            
            // Simulate AI processing animation
            const aiAgents = document.querySelectorAll('.ai-agent');
            
            aiAgents.forEach(agent => {
                const progressBar = agent.querySelector('.progress-bar');
                const randomWidth = Math.floor(Math.random() * 30) + 40;
                progressBar.style.width = `${randomWidth}%`;
                
                // Simulate processing animation
                setInterval(() => {
                    const currentWidth = parseInt(progressBar.style.width);
                    const newWidth = Math.min(100, currentWidth + Math.random() * 5);
                    progressBar.style.width = `${newWidth}%`;
                    
                    if (newWidth >= 95) {
                        progressBar.style.width = '40%';
                    }
                }, 3000);
            });
            
            // Update scraper status randomly
            setInterval(() => {
                const statuses = document.querySelectorAll('.scraper-status');
                statuses.forEach(status => {
                    if (Math.random() > 0.1) { // 90% chance to be online
                        status.classList.remove('offline');
                        status.classList.add('online');
                    } else {
                        status.classList.remove('online');
                        status.classList.add('offline');
                    }
                });
            }, 5000);
        }

        // Team management functions
        function renderTeams() {
            const teamsList = document.getElementById('teamsList');
            teamsList.innerHTML = '';
            
            if (appState.teams.length === 0) {
                teamsList.innerHTML = '<div class="col-12 text-center py-4"><p>No teams added yet. Click "Add Team" to get started.</p></div>';
                return;
            }
            
            appState.teams.forEach(team => {
                const teamCard = document.createElement('div');
                teamCard.className = 'col-md-6 col-lg-4 mb-3';
                teamCard.innerHTML = `
                    <div class="card h-100">
                        <div class="card-body">
                            <div class="d-flex align-items-center mb-3">
                                <img src="${team.logo}" class="team-logo" alt="${team.name}" onerror="this.src='https://via.placeholder.com/50x50/1a3a5f/ffffff?text=${team.name.substring(0,2)}'">
                                <h5 class="card-title mb-0">${team.name}</h5>
                            </div>
                            <p class="card-text">
                                <small class="text-muted">
                                    <i class="fas fa-trophy me-1"></i> ${team.league}<br>
                                    <i class="fas fa-flag me-1"></i> ${team.country}
                                </small>
                            </p>
                        </div>
                        <div class="card-footer bg-transparent">
                            <button class="btn btn-sm btn-outline-primary me-1" onclick="editTeam(${team.id})">
                                <i class="fas fa-edit"></i>
                            </button>
                            <button class="btn btn-sm btn-outline-danger" onclick="removeTeam(${team.id})">
                                <i class="fas fa-trash"></i>
                            </button>
                        </div>
                    </div>
                `;
                teamsList.appendChild(teamCard);
            });
        }

        function addTeam(name, country, league, logo) {
            // Default logo if none provided
            if (!logo) {
                const initials = name.split(' ').map(word => word[0]).join('').substring(0, 2);
                logo = `https://via.placeholder.com/50x50/1a3a5f/ffffff?text=${initials}`;
            }
            
            const newTeam = {
                id: Date.now(), // Simple ID generation
                name: name,
                country: country,
                league: league,
                logo: logo
            };
            
            // Add to teams array
            appState.teams.push(newTeam);
            
            // Save to localStorage
            localStorage.setItem('teams', JSON.stringify(appState.teams));
            
            // Update UI
            renderTeams();
            populateTeamSelects();
            
            // Show success message
            showNotification(`Team "${name}" added successfully!`, 'success');
        }

        function removeTeam(teamId) {
            if (confirm('Are you sure you want to remove this team?')) {
                // Filter out the team to remove
                appState.teams = appState.teams.filter(team => team.id !== teamId);
                
                // Save to localStorage
                localStorage.setItem('teams', JSON.stringify(appState.teams));
                
                // Update UI
                renderTeams();
                populateTeamSelects();
                
                // Show success message
                showNotification('Team removed successfully!', 'success');
            }
        }

        function editTeam(teamId) {
            // Find the team
            const team = appState.teams.find(t => t.id === teamId);
            if (!team) return;
            
            // Populate the form
            document.getElementById('teamName').value = team.name;
            document.getElementById('teamCountry').value = team.country;
            document.getElementById('teamLeague').value = team.league;
            document.getElementById('teamLogo').value = team.logo;
            
            // Change modal title and button
            document.getElementById('addTeamModalLabel').textContent = 'Edit Team';
            document.getElementById('saveTeamBtn').textContent = 'Update Team';
            document.getElementById('saveTeamBtn').onclick = function() {
                updateTeam(teamId);
            };
            
            // Show modal
            const modal = new bootstrap.Modal(document.getElementById('addTeamModal'));
            modal.show();
        }

        function updateTeam(teamId) {
            const teamName = document.getElementById('teamName').value;
            const teamCountry = document.getElementById('teamCountry').value;
            const teamLeague = document.getElementById('teamLeague').value;
            const teamLogo = document.getElementById('teamLogo').value;
            
            if (teamName && teamCountry && teamLeague) {
                // Find and update the team
                const teamIndex = appState.teams.findIndex(t => t.id === teamId);
                if (teamIndex !== -1) {
                    appState.teams[teamIndex].name = teamName;
                    appState.teams[teamIndex].country = teamCountry;
                    appState.teams[teamIndex].league = teamLeague;
                    appState.teams[teamIndex].logo = teamLogo || appState.teams[teamIndex].logo;
                    
                    // Save to localStorage
                    localStorage.setItem('teams', JSON.stringify(appState.teams));
                    
                    // Update UI
                    renderTeams();
                    populateTeamSelects();
                    
                    // Close modal
                    const modal = bootstrap.Modal.getInstance(document.getElementById('addTeamModal'));
                    modal.hide();
                    
                    // Reset form and button
                    document.getElementById('addTeamForm').reset();
                    document.getElementById('addTeamModalLabel').textContent = 'Add New Team';
                    document.getElementById('saveTeamBtn').textContent = 'Save Team';
                    document.getElementById('saveTeamBtn').onclick = function() {
                        const teamName = document.getElementById('teamName').value;
                        const teamCountry = document.getElementById('teamCountry').value;
                        const teamLeague = document.getElementById('teamLeague').value;
                        const teamLogo = document.getElementById('teamLogo').value;
                        
                        if (teamName && teamCountry && teamLeague) {
                            addTeam(teamName, teamCountry, teamLeague, teamLogo);
                            const modal = bootstrap.Modal.getInstance(document.getElementById('addTeamModal'));
                            modal.hide();
                            document.getElementById('addTeamForm').reset();
                        } else {
                            alert('Please fill in all required fields');
                        }
                    };
                    
                    // Show success message
                    showNotification(`Team "${teamName}" updated successfully!`, 'success');
                }
            } else {
                alert('Please fill in all required fields');
            }
        }

        function filterTeams() {
            const searchTerm = document.getElementById('teamSearch').value.toLowerCase();
            const leagueFilter = document.getElementById('leagueFilter').value;
            const countryFilter = document.getElementById('countryFilter').value;
            
            const filteredTeams = appState.teams.filter(team => {
                const matchesSearch = team.name.toLowerCase().includes(searchTerm);
                const matchesLeague = !leagueFilter || team.league === leagueFilter;
                const matchesCountry = !countryFilter || team.country === countryFilter;
                
                return matchesSearch && matchesLeague && matchesCountry;
            });
            
            const teamsList = document.getElementById('teamsList');
            teamsList.innerHTML = '';
            
            if (filteredTeams.length === 0) {
                teamsList.innerHTML = '<div class="col-12 text-center py-4"><p>No teams match your filters.</p></div>';
                return;
            }
            
            filteredTeams.forEach(team => {
                const teamCard = document.createElement('div');
                teamCard.className = 'col-md-6 col-lg-4 mb-3';
                teamCard.innerHTML = `
                    <div class="card h-100">
                        <div class="card-body">
                            <div class="d-flex align-items-center mb-3">
                                <img src="${team.logo}" class="team-logo" alt="${team.name}" onerror="this.src='https://via.placeholder.com/50x50/1a3a5f/ffffff?text=${team.name.substring(0,2)}'">
                                <h5 class="card-title mb-0">${team.name}</h5>
                            </div>
                            <p class="card-text">
                                <small class="text-muted">
                                    <i class="fas fa-trophy me-1"></i> ${team.league}<br>
                                    <i class="fas fa-flag me-1"></i> ${team.country}
                                </small>
                            </p>
                        </div>
                        <div class="card-footer bg-transparent">
                            <button class="btn btn-sm btn-outline-primary me-1" onclick="editTeam(${team.id})">
                                <i class="fas fa-edit"></i>
                            </button>
                            <button class="btn btn-sm btn-outline-danger" onclick="removeTeam(${team.id})">
                                <i class="fas fa-trash"></i>
                            </button>
                        </div>
                    </div>
                `;
                teamsList.appendChild(teamCard);
            });
        }

        function populateTeamSelects() {
            const homeTeamSelect = document.getElementById('homeTeam');
            const awayTeamSelect = document.getElementById('awayTeam');
            
            // Clear existing options except the first one
            while (homeTeamSelect.options.length > 1) {
                homeTeamSelect.remove(1);
            }
            while (awayTeamSelect.options.length > 1) {
                awayTeamSelect.remove(1);
            }
            
            // Add teams to selects
            appState.teams.forEach(team => {
                const homeOption = document.createElement('option');
                homeOption.value = team.name;
                homeOption.textContent = team.name;
                homeTeamSelect.appendChild(homeOption);
                
                const awayOption = document.createElement('option');
                awayOption.value = team.name;
                awayOption.textContent = team.name;
                awayTeamSelect.appendChild(awayOption);
            });
        }

        // Prediction management functions
        function renderPredictionsTable() {
            const predictionsTable = document.getElementById('predictionsTable');
            predictionsTable.innerHTML = '';
            
            if (appState.predictions.length === 0) {
                predictionsTable.innerHTML = '<tr><td colspan="8" class="text-center py-4">No predictions available.</td></tr>';
                return;
            }
            
            appState.predictions.forEach(prediction => {
                const row = document.createElement('tr');
                row.innerHTML = `
                    <td>
                        <div class="d-flex align-items-center">
                            <img src="${getTeamLogo(prediction.homeTeam)}" class="team-logo me-1" alt="${prediction.homeTeam}" onerror="this.src='https://via.placeholder.com/25x25/1a3a5f/ffffff?text=${prediction.homeTeam.substring(0,2)}'">
                            ${prediction.homeTeam} vs 
                            <img src="${getTeamLogo(prediction.awayTeam)}" class="team-logo ms-1 me-1" alt="${prediction.awayTeam}" onerror="this.src='https://via.placeholder.com/25x25/2a5a8c/ffffff?text=${prediction.awayTeam.substring(0,2)}'">
                            ${prediction.awayTeam}
                        </div>
                    </td>
                    <td>${prediction.league}</td>
                    <td>${new Date(prediction.date).toLocaleString()}</td>
                    <td>
                        <span class="prediction-badge ${getPredictionBadgeClass(prediction.prediction)}">
                            ${prediction.prediction}
                        </span>
                    </td>
                    <td>
                        <div class="progress" style="height: 8px;">
                            <div class="progress-bar ${getConfidenceClass(prediction.confidence)}" style="width: ${prediction.confidence}%"></div>
                        </div>
                        <small>${prediction.confidence}%</small>
                    </td>
                    <td>
                        <span class="prediction-badge ${prediction.btts === 'Yes' ? 'win-badge' : 'loss-badge'}">
                            ${prediction.btts}
                        </span>
                    </td>
                    <td>
                        <div class="d-flex">
                            <span class="prediction-badge ${prediction.prediction === 'Home Win' ? 'win-badge' : 'loss-badge'} me-1">${prediction.homeOdds}</span>
                            <span class="prediction-badge ${prediction.prediction === 'Draw' ? 'draw-badge' : 'loss-badge'} me-1">${prediction.drawOdds}</span>
                            <span class="prediction-badge ${prediction.prediction === 'Away Win' ? 'win-badge' : 'loss-badge'}">${prediction.awayOdds}</span>
                        </div>
                    </td>
                    <td>
                        <span class="badge ${prediction.status === 'Pending' ? 'bg-warning' : prediction.status === 'Correct' ? 'bg-success' : 'bg-danger'}">
                            ${prediction.status}
                        </span>
                    </td>
                `;
                predictionsTable.appendChild(row);
            });
        }

        function getTeamLogo(teamName) {
            const team = appState.teams.find(t => t.name === teamName);
            return team ? team.logo : `https://via.placeholder.com/25x25/1a3a5f/ffffff?text=${teamName.substring(0,2)}`;
        }

        function getPredictionBadgeClass(prediction) {
            switch(prediction) {
                case 'Home Win':
                case 'Away Win':
                    return 'win-badge';
                case 'Draw':
                    return 'draw-badge';
                default:
                    return 'loss-badge';
            }
        }

        function getConfidenceClass(confidence) {
            if (confidence >= 80) return 'bg-success';
            if (confidence >= 60) return 'bg-warning';
            return 'bg-danger';
        }

        function addMatchPrediction(homeTeam, awayTeam, league, date) {
            // Generate a random prediction (in a real app, this would use AI)
            const outcomes = ['Home Win', 'Away Win', 'Draw'];
            const randomOutcome = outcomes[Math.floor(Math.random() * outcomes.length)];
            const confidence = Math.floor(Math.random() * 30) + 60; // 60-90%
            const btts = Math.random() > 0.5 ? 'Yes' : 'No';
            
            // Generate realistic odds based on prediction
            let homeOdds, drawOdds, awayOdds;
            if (randomOutcome === 'Home Win') {
                homeOdds = (1.2 + Math.random() * 0.8).toFixed(2);
                drawOdds = (3.0 + Math.random() * 1.5).toFixed(2);
                awayOdds = (4.0 + Math.random() * 2.0).toFixed(2);
            } else if (randomOutcome === 'Away Win') {
                homeOdds = (4.0 + Math.random() * 2.0).toFixed(2);
                drawOdds = (3.0 + Math.random() * 1.5).toFixed(2);
                awayOdds = (1.2 + Math.random() * 0.8).toFixed(2);
            } else {
                homeOdds = (2.5 + Math.random() * 1.5).toFixed(2);
                drawOdds = (2.0 + Math.random() * 1.0).toFixed(2);
                awayOdds = (2.5 + Math.random() * 1.5).toFixed(2);
            }
            
            const newPrediction = {
                id: Date.now(),
                homeTeam: homeTeam,
                awayTeam: awayTeam,
                league: league,
                date: date,
                prediction: randomOutcome,
                confidence: confidence,
                btts: btts,
                homeOdds: parseFloat(homeOdds),
                drawOdds: parseFloat(drawOdds),
                awayOdds: parseFloat(awayOdds),
                status: 'Pending'
            };
            
            // Add to predictions array
            appState.predictions.push(newPrediction);
            
            // Save to localStorage
            localStorage.setItem('predictions', JSON.stringify(appState.predictions));
            
            // Update UI
            renderPredictionsTable();
            
            // Show success message
            showNotification(`Prediction for ${homeTeam} vs ${awayTeam} generated successfully!`, 'success');
        }

        // Refresh data function
        function refreshData() {
            // Show loading state
            const refreshBtn = document.getElementById('refreshDataBtn');
            const originalText = refreshBtn.innerHTML;
            refreshBtn.innerHTML = '<i class="fas fa-spinner fa-spin me-1"></i> Refreshing...';
            refreshBtn.disabled = true;
            
            // Simulate API calls to refresh data
            setTimeout(() => {
                // Update last refresh time
                appState.lastRefresh = new Date();
                document.getElementById('lastRefreshTime').textContent = 'Just now';
                
                // Restore button state
                refreshBtn.innerHTML = originalText;
                refreshBtn.disabled = false;
                
                // Show success message
                showNotification('Data refreshed successfully!', 'success');
                
                // In a real app, you would update the predictions and teams from APIs here
            }, 2000);
        }

        // Auto-refresh setup
        function setupAutoRefresh() {
            const autoRefreshCheckbox = document.getElementById('autoRefresh');
            const refreshIntervalSelect = document.getElementById('refreshInterval');
            
            let refreshInterval;
            
            function startAutoRefresh() {
                const interval = parseInt(refreshIntervalSelect.value) * 60 * 1000; // Convert to milliseconds
                refreshInterval = setInterval(refreshData, interval);
            }
            
            function stopAutoRefresh() {
                if (refreshInterval) {
                    clearInterval(refreshInterval);
                }
            }
            
            autoRefreshCheckbox.addEventListener('change', function() {
                if (this.checked) {
                    startAutoRefresh();
                } else {
                    stopAutoRefresh();
                }
            });
            
            refreshIntervalSelect.addEventListener('change', function() {
                if (autoRefreshCheckbox.checked) {
                    stopAutoRefresh();
                    startAutoRefresh();
                }
            });
            
            // Start auto-refresh if enabled
            if (autoRefreshCheckbox.checked) {
                startAutoRefresh();
            }
        }

        // Notification system
        function showNotification(message, type = 'info') {
            // Create notification element
            const notification = document.createElement('div');
            notification.className = `alert alert-${type} alert-dismissible fade show position-fixed`;
            notification.style.cssText = 'top: 20px; right: 20px; z-index: 1050; min-width: 300px;';
            notification.innerHTML = `
                ${message}
                <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
            `;
            
            // Add to page
            document.body.appendChild(notification);
            
            // Auto remove after 5 seconds
            setTimeout(() => {
                if (notification.parentNode) {
                    notification.parentNode.removeChild(notification);
                }
            }, 5000);
        }

