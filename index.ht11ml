                <div class="disclaimer">
                    <p><strong>Disclaimer:</strong> This website is for demonstration purposes only. Predictions are simulated and not based on real-time data. Always gamble responsibly.</p>
                </div>
                <p>&copy; 2023 AI Soccer Predictions. All rights reserved.</p>
            </div>
        </div>
    </footer>

    <script>
        // Simple script for mobile menu toggle
        document.querySelector('.mobile-menu').addEventListener('click', function() {
            document.querySelector('nav ul').classList.toggle('show');
        });
        
        // Simulate AI processing
        document.addEventListener('DOMContentLoaded', function() {
            const statusElement = document.querySelector('.ai-status span');
            const originalText = statusElement.textContent;
            
            // Simulate AI processing states
            const states = [
                "AI Prediction Engine: Analyzing Team Form...",
                "AI Prediction Engine: Comparing Odds Data...",
                "AI Prediction Engine: Calculating Probabilities...",
                "AI Prediction Engine: Generating Predictions...",
                "AI Prediction Engine: Active & Analyzing Today's Matches"
            ];
            
            let currentState = 0;
            const interval = setInterval(() => {
                statusElement.textContent = states[currentState];
                currentState = (currentState + 1) % states.length;
                
                if (currentState === states.length - 1) {
                    clearInterval(interval);
                    setTimeout(() => {
                        statusElement.textContent = originalText;
                    }, 2000);
                }
            }, 1500);
        });
    </script>
</body>
</html>
