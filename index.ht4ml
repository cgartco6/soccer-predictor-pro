                `;
                
                // 180Score predictions
                document.getElementById('f180score-predictions').innerHTML = `
                    <div class="prediction-result">
                        <span>Man City vs Liverpool</span>
                        <span class="prediction-value">2-1 (Home Win)</span>
                    </div>
                    <div class="prediction-result">
                        <span>Real Madrid vs Barcelona</span>
                        <span class="prediction-value">2-2 (Draw)</span>
                    </div>
                    <div class="prediction-result">
                        <span>Juventus vs AC Milan</span>
                        <span class="prediction-value">1-0 (Home Win)</span>
                    </div>
                    <div class="prediction-result">
                        <span>Bayern vs Dortmund</span>
                        <span class="prediction-value">3-2 (Home Win)</span>
                    </div>
                `;
            }, 2000);
        }

        // Load prediction history
        function loadPredictionHistory() {
            const history = [
                { match: 'Man City vs Arsenal', prediction: 'Win & BTTS', result: '3-1', status: 'correct', date: '2023-10-15' },
                { match: 'Liverpool vs Chelsea', prediction: 'Draw', result: '2-0', status: 'incorrect', date: '2023-10-14' },
                { match: 'Barcelona vs Real Madrid', prediction: 'Over 2.5', result: '3-2', status: 'correct', date: '2023-10-12' },
                { match: 'Bayern vs Dortmund', prediction: 'Win', result: '4-0', status: 'correct', date: '2023-10-10' },
                { match: 'PSG vs Marseille', prediction: 'BTTS', result: '1-0', status: 'incorrect', date: '2023-10-08' }
            ];
            
            const historyBody = document.getElementById('history-body');
            historyBody.innerHTML = '';
            
            history.forEach(item => {
                const row = document.createElement('tr');
                row.className = item.status === 'correct' ? 'correct' : 'incorrect';
                
                row.innerHTML = `
                    <td>${item.match}</td>
                    <td>${item.prediction}</td>
                    <td>${item.result}</td>
                    <td class="status-${item.status}">${item.status === 'correct' ? 'Correct' : 'Incorrect'}</td>
                    <td>${item.date}</td>
                `;
                
                historyBody.appendChild(row);
            });
        }

        // Generate predictions based on team names
        function generatePredictions(homeTeam, awayTeam, league) {
            const homeStrength = calculateTeamStrength(homeTeam);
            const awayStrength = calculateTeamStrength(awayTeam);
            const predictions = generateMatchPredictions(homeTeam, awayTeam, homeStrength, awayStrength);
            
            // Update our predictions
            document.getElementById('our-match-result').textContent = predictions.matchResult;
            document.getElementById('our-btts').textContent = predictions.btts;
            document.getElementById('our-matchbet-btts').textContent = predictions.matchbetBtts;
            document.getElementById('our-confidence').textContent = `${predictions.confidence}%`;
            
            // Generate Betway predictions (simulated scraping)
            const betwayPredictions = generateWebsitePrediction(homeTeam, awayTeam, homeStrength, awayStrength, 'betway');
            document.getElementById('betway-match-result').textContent = betwayPredictions.matchResult;
            document.getElementById('betway-btts').textContent = betwayPredictions.btts;
            document.getElementById('betway-matchbet-btts').textContent = betwayPredictions.matchbetBtts;
            
            // Generate Hollywood Bets predictions (simulated scraping)
            const hollywoodPredictions = generateWebsitePrediction(homeTeam, awayTeam, homeStrength, awayStrength, 'hollywood');
            document.getElementById('hollywood-match-result').textContent = hollywoodPredictions.matchResult;
            document.getElementById('hollywood-btts').textContent = hollywoodPredictions.btts;
            document.getElementById('hollywood-matchbet-btts').textContent = hollywoodPredictions.matchbetBtts;
            
            // Create prediction comparison chart
            createPredictionChart(predictions, betwayPredictions, hollywoodPredictions);
        }

        // Generate match predictions
        function generateMatchPredictions(homeTeam, awayTeam, homeStrength, awayStrength) {
            const homeAdvantage = 0.1; // 10% home advantage
            
            // Calculate probabilities
            const homeWinProb = (homeStrength + homeAdvantage) / (homeStrength + awayStrength + homeAdvantage);
            const awayWinProb = awayStrength / (homeStrength + awayStrength + homeAdvantage);
            const drawProb = 1 - homeWinProb - awayWinProb;
            
            // Determine match result prediction
            let matchResult, confidence;
            if (homeWinProb > awayWinProb && homeWinProb > drawProb) {
                matchResult = `${homeTeam} Win`;
                confidence = Math.round(homeWinProb * 100);
            } else if (awayWinProb > homeWinProb && awayWinProb > drawProb) {
                matchResult = `${awayTeam} Win`;
                confidence = Math.round(awayWinProb * 100);
            } else {
                matchResult = 'Draw';
                confidence = Math.round(drawProb * 100);
            }
            
            // Determine BTTS probability (based on team attacking/defensive strengths)
            const bttsProb = 0.5 + (homeStrength * 0.2) + (awayStrength * 0.2);
            const bttsPrediction = bttsProb > 0.5 ? 'Yes' : 'No';
            
            // Determine Matchbet & BTTS prediction
            let matchbetBtts;
            if (matchResult.includes('Win') && bttsPrediction === 'Yes') {
                matchbetBtts = `${matchResult} & BTTS`;
            } else {
                matchbetBtts = 'Not Recommended';
            }
            
            return {
                matchResult,
                btts: bttsPrediction,
                matchbetBtts,
                confidence
            };
        }

        // Calculate team strength based on team name (simplified)
        function calculateTeamStrength(teamName) {
            // In a real app, this would use actual team data from APIs
            // For demo, we'll use a simple algorithm based on name length and vowels
            const baseStrength = 5;
            const nameFactor = teamName.length * 0.1;
            const vowelCount = (teamName.match(/[aeiou]/gi) || []).length;
            const vowelFactor = vowelCount * 0.05;
            
            return Math.min(10, baseStrength + nameFactor + vowelFactor);
        }

        // Generate website-specific predictions
        function generateWebsitePrediction(homeTeam, awayTeam, homeStrength, awayStrength, website) {
            // Each website has slightly different algorithms
            let homeWinProb, awayWinProb, drawProb;
            
            switch(website) {
                case 'betway':
                    homeWinProb = (homeStrength + 0.05) / (homeStrength + awayStrength + 0.1);
                    awayWinProb = (awayStrength - 0.05) / (homeStrength + awayStrength + 0.1);
                    drawProb = 1 - homeWinProb - awayWinProb;
                    break;
                case 'hollywood':
                    homeWinProb = (homeStrength - 0.05) / (homeStrength + awayStrength - 0.1);
                    awayWinProb = (awayStrength + 0.05) / (homeStrength + awayStrength - 0.1);
                    drawProb = 1 - homeWinProb - awayWinProb;
                    break;
                default:
                    homeWinProb = homeStrength / (homeStrength + awayStrength);
                    awayWinProb = awayStrength / (homeStrength + awayStrength);
                    drawProb = 1 - homeWinProb - awayWinProb;
            }
            
            let matchResult;
            if (homeWinProb > awayWinProb && homeWinProb > drawProb) {
                matchResult = `${homeTeam} Win`;
            } else if (awayWinProb > homeWinProb && awayWinProb > drawProb) {
                matchResult = `${awayTeam} Win`;
            } else {
                matchResult = 'Draw';
            }
            
            // BTTS prediction
            const bttsProb = 0.5 + (homeStrength * 0.15) + (awayStrength * 0.15);
            const btts = bttsProb > 0.5 ? 'Yes' : 'No';
            
            // Matchbet & BTTS
            let matchbetBtts;
            if (matchResult.includes('Win') && btts === 'Yes') {
                matchbetBtts = `${matchResult} & BTTS`;
            } else {
                matchbetBtts = 'Not Recommended';
            }
            
            return {
                matchResult,
                btts,
                matchbetBtts
            };
        }

        // Create prediction comparison chart
        function createPredictionChart(ourPredictions, betwayPredictions, hollywoodPredictions) {
            const ctx = document.getElementById('prediction-chart').getContext('2d');
            
            // Convert predictions to numerical values for chart
            const ourValue = ourPredictions.confidence;
            const betwayValue = Math.floor(Math.random() * 20) + 70; // Random between 70-90
            const hollywoodValue = Math.floor(Math.random() * 20) + 65; // Random between 65-85
            
            new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: ['Our Algorithm', 'Betway', 'Hollywood Bets'],
                    datasets: [{
                        label: 'Prediction Confidence %',
                        data: [ourValue, betwayValue, hollywoodValue],
                        backgroundColor: [
                            'rgba(46, 125, 50, 0.7)',
                            'rgba(0, 166, 81, 0.7)',
                            'rgba(230, 0, 126, 0.7)'
                        ],
                        borderColor: [
                            'rgb(46, 125, 50)',
                            'rgb(0, 166, 81)',
                            'rgb(230, 0, 126)'
                        ],
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 100,
                            title: {
                                display: true,
                                text: 'Confidence %'
                            }
                        }
                    }
                }
            });
        }

        // Helper functions
        function getTeamAbbreviation(teamName) {
            return teamName.split(' ').map(word => word[0]).join('').toUpperCase().substring(0, 3);
        }

        function formatMatchTime(dateString) {
            const date = new Date(dateString);
            return date.toLocaleTimeString([], { hour: '2-digit', minute: '2-digit' });
        }

        // Smooth scrolling for navigation links
        document.querySelectorAll('nav a').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                
                const targetId = this.getAttribute('href');
                if(targetId !== '#') {
                    const targetElement = document.querySelector(targetId);
                    window.scrollTo({
                        top: targetElement.offsetTop - 80,
                        behavior: 'smooth'
                    });
                }
            });
        });

        // Initialize the page with data
        document.addEventListener('DOMContentLoaded', function() {
            fetchLiveData();
        });
    </script>
