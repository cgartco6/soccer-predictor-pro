        // Fetch API data button
        document.getElementById('fetch-api-data').addEventListener('click', function() {
            fetchLiveData();
        });

        // Custom team analysis
        document.getElementById('analyze-btn').addEventListener('click', function() {
            const homeTeam = document.getElementById('home-team').value;
            const awayTeam = document.getElementById('away-team').value;
            const league = document.getElementById('league').value;
            
            if(homeTeam && awayTeam && league) {
                // Show results section
                document.getElementById('prediction-results').style.display = 'block';
                
                // Generate realistic predictions based on team names
                generatePredictions(homeTeam, awayTeam, league);
                
                // Scroll to results
                document.getElementById('prediction-results').scrollIntoView({ behavior: 'smooth' });
            } else {
                alert('Please enter both team names and select a league');
            }
        });

        // Clear form
        document.getElementById('clear-btn').addEventListener('click', function() {
            document.getElementById('home-team').value = '';
            document.getElementById('away-team').value = '';
            document.getElementById('league').value = '';
            document.getElementById('match-date').value = '';
            document.getElementById('prediction-results').style.display = 'none';
        });

        // Fetch live data from APIs
        async function fetchLiveData() {
            // Show loading
            document.getElementById('loading-predictions').style.display = 'block';
            document.getElementById('predictions-container').innerHTML = '';
            
            try {
                let matches;
                
                if (API_CONFIG.mockData) {
                    // Use mock data for demonstration
                    matches = await fetchMockMatchData();
                } else {
                    // In a real application, you would fetch from actual APIs
                    // matches = await fetchFootballDataMatches();
                }
                
                // Render matches
                renderMatches(matches);
                
                // Fetch betting site predictions
                fetchBettingSitePredictions();
                
                // Load prediction history
                loadPredictionHistory();
                
            } catch (error) {
                console.error('Error fetching data:', error);
                document.getElementById('predictions-container').innerHTML = `
                    <div class="card">
                        <div class="card-body">
                            <p>Error loading match data. Please try again later.</p>
                        </div>
                    </div>
                `;
            } finally {
                document.getElementById('loading-predictions').style.display = 'none';
            }
        }

        // Fetch mock match data (simulating API response)
        async function fetchMockMatchData() {
            // Simulate API delay
            await new Promise(resolve => setTimeout(resolve, 1500));
            
            return [
                {
                    id: 1,
                    competition: 'Premier League',
                    homeTeam: 'Manchester City',
                    awayTeam: 'Liverpool',
                    date: '2023-11-05T15:00:00Z',
                    venue: 'Etihad Stadium'
                },
                {
                    id: 2,
                    competition: 'La Liga',
                    homeTeam: 'Real Madrid',
                    awayTeam: 'Barcelona',
                    date: '2023-11-05T20:00:00Z',
                    venue: 'Santiago BernabÃ©u'
                },
                {
                    id: 3,
                    competition: 'Serie A',
                    homeTeam: 'Juventus',
                    awayTeam: 'AC Milan',
                    date: '2023-11-05T18:30:00Z',
                    venue: 'Allianz Stadium'
                },
                {
                    id: 4,
                    competition: 'Bundesliga',
                    homeTeam: 'Bayern Munich',
                    awayTeam: 'Borussia Dortmund',
                    date: '2023-11-04T17:30:00Z',
                    venue: 'Allianz Arena'
                }
            ];
        }

        // Render matches to the DOM
        function renderMatches(matches) {
            const container = document.getElementById('predictions-container');
            
            matches.forEach(match => {
                const card = document.createElement('div');
                card.className = 'card';
                
                // Generate predictions for this match
                const homeStrength = calculateTeamStrength(match.homeTeam);
                const awayStrength = calculateTeamStrength(match.awayTeam);
                const predictions = generateMatchPredictions(match.homeTeam, match.awayTeam, homeStrength, awayStrength);
                
                card.innerHTML = `
                    <div class="card-header">${match.competition}</div>
                    <div class="card-body">
                        <div class="match-info">
                            <div class="teams">
                                <div class="team">
                                    <div class="team-logo">${getTeamAbbreviation(match.homeTeam)}</div>
                                    <span>${match.homeTeam}</span>
                                </div>
                                <div class="vs">VS</div>
                                <div class="team">
                                    <div class="team-logo">${getTeamAbbreviation(match.awayTeam)}</div>
                                    <span>${match.awayTeam}</span>
                                </div>
                            </div>
                            <div class="match-time">${formatMatchTime(match.date)}</div>
                        </div>
                        <div class="prediction-options">
                            <div class="prediction-option ${predictions.matchResult.includes('Win') ? 'selected' : ''}">${predictions.matchResult}</div>
                            <div class="prediction-option ${predictions.btts === 'Yes' ? 'selected' : ''}">BTTS ${predictions.btts}</div>
                            <div class="prediction-option ${predictions.matchbetBtts !== 'Not Recommended' ? 'selected' : ''}">${predictions.matchbetBtts}</div>
                        </div>
                        <div class="accuracy-meter">
                            <div class="meter-bar">
                                <div class="meter-fill" style="width: ${predictions.confidence}%"></div>
                            </div>
                            <div class="accuracy-text">
                                <span>Confidence</span>
                                <span>${predictions.confidence}%</span>
                            </div>
                        </div>
                    </div>
                `;
                
                container.appendChild(card);
            });
        }

        // Fetch betting site predictions (simulated scraping)
        async function fetchBettingSitePredictions() {
            // Simulate scraping delay
            setTimeout(() => {
                // Betway predictions
                document.getElementById('betway-predictions').innerHTML = `
                    <div class="prediction-result">
                        <span>Man City vs Liverpool</span>
                        <span class="prediction-value">Home Win & BTTS</span>
                    </div>
                    <div class="prediction-result">
                        <span>Real Madrid vs Barcelona</span>
                        <span class="prediction-value">Draw & BTTS</span>
                    </div>
                    <div class="prediction-result">
                        <span>Juventus vs AC Milan</span>
